{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "first_name": "Sam",
            "last_name": "Lee",
            "location": "place",
            "gender": "m",
            "phone_num": "1234567980",
            "want_buddy": true,
            "has_buddy": false,
            "buddy_email": "friend@gmail.com",
            "_token": "stringwithalotofcharacters"
        }
    ],
    "required": [
    ],
    "properties": {
        "first_name": {
            "$id": "#/properties/first_name",
            "type": "string",
            "title": "The first_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Sam"
            ]
        },
        "last_name": {
            "$id": "#/properties/last_name",
            "type": "string",
            "title": "The last_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Lee"
            ]
        },
        "location": {
            "$id": "#/properties/location",
            "type": "string",
            "title": "The location schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "place"
            ]
        },
        "gender": {
            "$id": "#/properties/gender",
            "type": "string",
            "title": "The gender schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "m"
            ]
        },
        "phone_num": {
            "$id": "#/properties/phone_num",
            "type": "string",
            "title": "The phone_num schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "1234567980"
            ]
        },
        "want_buddy": {
            "$id": "#/properties/want_buddy",
            "type": "boolean",
            "title": "The want_buddy schema",
            "description": "An explanation about the purpose of this instance.",
            "default": false,
            "examples": [
                true
            ]
        },
        "has_buddy": {
            "$id": "#/properties/has_buddy",
            "type": "boolean",
            "title": "The has_buddy schema",
            "description": "An explanation about the purpose of this instance.",
            "default": false,
            "examples": [
                false
            ]
        },
        "buddy_email": {
            "$id": "#/properties/buddy_email",
            "type": "string",
            "format":"email",
            "title": "The buddy_email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "friend@gmail.com"
            ]
        },
        "_token": {
            "$id": "#/properties/_token",
            "type": "string",
            "title": "The _token schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "stringwithalotofcharacters"
            ]
        }
    },
    "additionalProperties": false
}